{
  "name": "@egose/swagger",
  "version": "0.0.0-PLACEHOLDER",
  "description": "OpenAPI (Swagger) integration of @egose/acl",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/egose/egose"
  },
  "homepage": "https://egose.github.io/egose-swagger/philosophy/",
  "keywords": [
    "express",
    "mongoose",
    "access control list",
    "api endpoints",
    "openapi",
    "swagger"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc -d",
    "bundle": "tsup",
    "lint": "git ls-files '*.js' '*.ts' | xargs yarn eslint"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@openapi-contrib/json-schema-to-openapi-schema": "^2.2.5",
    "lodash": "^4.17.21",
    "swagger-ui-express": "^4.6.2"
  },
  "peerDependencies": {
    "@egose/acl": "0.0.0-PLACEHOLDER",
    "express": ">=4.6.0",
    "mongoose": ">=5.2.0"
  },
  "tsup": {
    "target": "node14",
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "minify": false
  }
}
